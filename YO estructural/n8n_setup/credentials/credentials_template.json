[
  {
    "name": "Neo4j Credentials",
    "type": "neo4j",
    "data": {
      "host": "${NEO4J_HOST}",
      "port": "${NEO4J_PORT}",
      "username": "${NEO4J_USER}",
      "password": "${NEO4J_PASSWORD}",
      "database": "${NEO4J_DATABASE}"
    }
  },
  {
    "name": "Basic Auth Local",
    "type": "httpBasicAuth",
    "data": {
      "user": "admin",
      "password": "${N8N_BASIC_AUTH_PASSWORD}"
    }
  },
  {
    "name": "API Key N8N",
    "type": "httpHeaderAuth",
    "data": {
      "name": "X-N8N-API-KEY",
      "value": "${N8N_API_KEY}"
    }
  }
]
